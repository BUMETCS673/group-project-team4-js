{
  "name": "rxcellent-global-packages",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "cz": "git add . && git-cz"
  },
  "devDependencies": {
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3"
  },
  "pre-commit": "lint",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  }
}
